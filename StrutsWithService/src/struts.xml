<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts  
Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	
	<package name="regPack" extends="struts-default">
		<action name="reg" class="action.RegistrationAction">
			<result name="success">/success.jsp</result>
			<result name="failure">/registerFailure.jsp</result>
			<result name="existed">/existed.jsp</result>
		</action>
	</package>
	
	<package name="loginPack" extends="struts-default">
		<action name="login" class="action.LoginAction">
			<result name="success">/loginSuccess.jsp</result>
			<result name="failure">/loginFailure.jsp</result>
		</action>
	</package>
	
	<package name="showUserPack" extends="struts-default">
		<action name="showUsers" class="action.ShowAllUserAction">
			<result name="success">/users.jsp</result>
			<result name="failure">/failure.jsp</result>
		</action>
	</package>

</struts>